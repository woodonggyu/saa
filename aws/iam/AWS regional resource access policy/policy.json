{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "RegionResourceAccessControl",
            "Effect": "Deny",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "StringNotEquals": {
                    "aws:RequestedRegion": "ap-northeast-2"
                }
            }
        }
    ]
}